############# miscellaneous #############

Host cable
    User root
    HostName 192.168.2.1
    CheckHostIP no
    StrictHostKeyChecking no

Host local
    User root
    HostName 10.0.0.1
    CheckHostIP no
    StrictHostKeyChecking no
    CertificateFile=~/.ssh/robot.crt
    IdentityFile=~/.ssh/robot.key

Host registry
    User root
    Hostname registry.kemaro.ch
    CheckHostIP no
    StrictHostKeyChecking no
    Port 2222

Host service
    User root
    Hostname localhost
    CertificateFile=~/.ssh/robot.crt
    IdentityFile=~/.ssh/robot.key
    Port 8083

Host wireguard
    HostName 10.42.42.2
    User root
    Port 22
    CertificateFile=~/.ssh/robot.crt
    IdentityFile=~/.ssh/robot.key

Host docker
    User root
    Hostname localhost
    CheckHostIP no
    StrictHostKeyChecking no
    Port 11312

Host github.com-work
    HostName github.com
    user git
    IdentityFile ~/.ssh/id_ed25519
    IdentitiesOnly yes

Host github.com-personal
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa

Host bitbucket.org
    AddressFamily inet
    HostName bitbucket.org
    user git
    IdentityFile ~/.ssh/id_rsa


