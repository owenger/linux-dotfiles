############# miscellaneous #############

Host cable
    User root
    HostName 192.168.2.1
    CheckHostIP no
    StrictHostKeyChecking no

Host local
    User root
    HostName 10.0.0.1
    CheckHostIP no
    StrictHostKeyChecking no

Host registry
    User root
    Hostname registry.kemaro.ch
    CheckHostIP no
    StrictHostKeyChecking no
    Port 2222

Host service
    User root
    Hostname localhost
    CheckHostIP no
    StrictHostKeyChecking no
    Port 8083

Host docker
    User root
    Hostname localhost
    CheckHostIP no
    StrictHostKeyChecking no
    Port 11312

Host github.com
    HostName github.com
    user git
    IdentityFile ~/.ssh/id_ed25519

Host bitbucket.org
    AddressFamily inet
    HostName bitbucket.org
    user git
    IdentityFile ~/.ssh/id_ed25519


